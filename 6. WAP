//6. WAP to accept a coordinate point in an XY coordinate system and determine in which quadrant the coordinate point lies

  int main() {
  int x,y;
  printf("Enter the x-coordinate = \n");
  scanf("%d",&x);
  printf("Enter the y-coordinate = \n");
  scanf("%d",&y);
  if(x>0 && y>0){
  printf("The coordinates (%d,%d) lies in the first quadrant\n",x,y);
}
  else if (x<0 && y>0) {
  printf("The coordinates (%d,%d) lies in the second quadrant \n",x,y);
}
  else if (x<0 && y<0){
  printf("The coordinates (%d,%d) lies in the third quadrant \n",x,y);
}
  else if (x>0 && y<0) {
  printf("The coordinates (%d,%d) lies in the fourth quadrant \n",x,y);
}
  else if (x==0 && y==0) {
  printf("The coordinates (%d,%d) lies at origin\n",x,y);
}
  return 0;}
